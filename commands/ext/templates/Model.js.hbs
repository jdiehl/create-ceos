const { Sequelize, sequelize } = require('ceos/sequelize')
const { User } = require('ceos/extensions/user')

class {{model}} extends Sequelize.Model {
}

{{model}}.init({
  name: {
    type: Sequelize.STRING,
    allowNull: false
  },
  user_id: {
    type: Sequelize.INTEGER,
    allowNull: false,
    references: { model: User }
  }
}, {
  sequelize,
  modelName: '{{camelCase model}}',
  indexes: [
  ]
})

exports.{{model}} = {{model}}
